.blog-post-container article .theme-default-content {
  margin-left: auto;
  margin-right: auto;
  width: 100%;

  @media (min-width: 768px) {
    max-width: 66.6666%;
  }

  > p:first-of-type > img:first-of-type {
    display: block;

    @media (min-width: 768px) {
      width: 114%;
      max-width: 114%;
      margin-left: -7%;
      margin-right: -7%;
    }
  }

  blockquote {
    background-color: #F3FAFD;
    padding: 40px 40px 30px;
    position: relative;
    margin: 30px 0;

    p {
      font-weight: 300;
      font-size: 24px;
      line-height: 138.5%;
      margin: 0;
      padding: 0;
    }

    &::before {
      content: '';
      background-image: url('/img/quote.svg');
      background-size: contain;
      width: 50px;
      height: 60px;
      color: $btnPrimaryBgColor;
      font-size: 100px;
      position: absolute;
      top: -16px;
      left: 22px;
    }
  }
}
