// Temporary scoping for home page changes
.home, .v-application nav {
  .btnPrimary, .btnSecondary, .btnText, .text-cta {
    font-family: $fontFamily;
    border-radius: 4px;
    font-size: 13px;
    font-weight: 800;
    text-transform: uppercase;
    padding: 14px 20px;
    letter-spacing: $buttonLetterSpacing;
    transition: all $hoverTransitionTime ease-in-out;
  }

  .btnPrimary, .btnSecondary {
    &:hover {
      transform: translateY(-1px);
      box-shadow: 1px 1px 8px 0 rgba(0, 0, 0, 0.09);
    }
  }

  .btnPrimary {
    color: $btnPrimaryTextColor;
    background-color: $btnPrimaryBgColor;

    &:hover {
      background-color: $blueBgColor;
    }
  }

  .btnSecondary {
    color: $btnSecondaryTextColor;
    background-color: $btnSecondaryBgColor;

    &:hover {
      background-color: $btnSecondaryHoverColor;
    }
  }

  .btnText {
    color: $btnSecondaryTextColor;
    background-color: transparent;

    &:hover {
      background-color: $btnSecondaryHoverColor;
    }
  }

  .text-cta {
    height: auto;
    padding: 0;
    text-decoration: underline;
    color: $btnPrimaryBgColor;

    &:hover {
      color: $textLinkColor;
    }
  }
}

.v-application nav {
  .btnPrimary {
    padding: 14px 21px;
    height: 37px;
    background-color: #1F6BF1;
  }

  .btnSecondary {
    background-color: $btnSecondaryHoverColor;

    &:hover {
      background-color: $borderColor;
    }
  }
}

.v-application code {
  font-size: 100% !important;
}

